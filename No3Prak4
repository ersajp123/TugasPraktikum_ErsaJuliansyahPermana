#include <iostream>
using namespace std;

int main (){
    int n, a=0, b=1, c;

    cout << "Masukkan jumlah elemen: ";
    cin >> n;

    cout << "Elemen bilangan dengan jumlah elemen "<< n <<" dalam Deret Fibonacci adalah : ";

    //Menampilkan dua angka pertama dari deret Fibonacci (0 dan 1)
    cout << a << " " << b << " ";

    //Menampilkan elemen-elemen berikutnya dalam deret Fibonacci
    for (int i = 2; i < n; i++) 
    {
        c = a + b;
        cout << c << " ";
        a = b;
        b = c;
    }

    cout << endl;

    return 0;

}
